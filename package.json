  "scripts": {
    "build": "SET NODE_ENV=production && webpack --config build/webpack.prod.config.js --mode production",
    "dev": "SET NODE_ENV=development && webpack-dev-server --open --config build/webpack.dev.config.js --mode development",
    "dll": "webpack --config build/webpack.dll.config.js --mode production",
    "start": "npm run dll && npm run dev",
    "pack": "node utils/pack.js",
    "prod": "npm run dll && npm run build && npm run pack"
